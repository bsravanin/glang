# TEST: List operations.

def list<bool> bool_list_ops(list<bool> lb):
	print lb.index(True)

	if lb.contains(True):
		print "lb contains True."
	else:
		print "lb doesn't contain True."

	lb.append(False)
	print lb
	print lb.index(False)
	lb.append(True)
	print lb
	lb.append(False)
	print lb

	list<bool> lb2 = lb.copy()
	print lb2

	lb.extend(lb2)
	print lb

	lb2.insert(1, True)
	print lb2
	lb2.pop(2)
	print lb2
	lb2.remove(True)
	print lb2
	
	for bool b in lb:
		print b

	return lb


def list<float> float_list_ops(list<float> lf):
	print lf.index(3.14)

	if lf.contains(3.14):
		print "lf contains 3.14."
	else:
		print "lf doesn't contain 3.14."

	lf.append(6.28)
	print lf
	print lf.index(6.28)
	lf.append(1.57)
	print lf
	lf.append(3.14)
	print lf

	list<float> lf2 = lf.copy()
	print lf2

	lf.extend(lf2)
	print lf

	lf2.insert(1, 42)
	print lf2
	lf2.pop(2)
	print lf2
	lf2.remove(3.14)
	print lf2
	
	for float f in lf:
		print f

	return lf


