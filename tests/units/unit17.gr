def list<str> str_list_ops(list<str> ls):
	print ls.index("Rosebud")

	if ls.contains("Rosebud"):
		print "ls contains Rosebud."
	else:
		print "ls doesn't contain Rosebud."

	ls.append("Citizen Kane")
	print ls
	print ls.index("Citizen Kane")
	ls.append("Orson Welles")
	print ls
	ls.append("Rosebud")
	print ls

	list<str> ls2 = ls.copy()
	print ls2

	ls.extend(ls2)
	print ls

	ls2.insert(1, "42")
	print ls2
	ls2.pop(2)
	print ls2
	ls2.remove("Rosebud")
	print ls2
	
	for str s in ls:
		print s

	return ls


def void main():
	list<bool> lb = [True]
	print bool_list_ops()

	list<float> lf = [3.14]
	print float_list_ops()

	list<int> li = [42]
	print int_list_ops()

	list<str> ls = ["Rosebud"]
	print str_list_ops()
